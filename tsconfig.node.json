{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.cache/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "target": "ESNext",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "types": ["node"],
    "lib": ["ESNext"]
  },
  "include": ["vite.config.ts", "scripts/**/*.ts"]
}
